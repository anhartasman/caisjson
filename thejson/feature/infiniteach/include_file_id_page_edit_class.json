        "catch": "id",
        "variable": "class_id"
    },
    {
        "type": "table",
        "from_engine": false,
        "runifnotnull": [],
        "table_name": "tb_class",
        "process_name": "select",
        "output_generate": "auto",
        "param": [
            {
                "var_name": "class_id"
            }
        ],
        "array_data": [
            "id",
            "class",
            "description",
            "class_start"
        ],
        "where": [
            {
                "index": "id",
                "operator": "=",
                "value": {
                    "var_name": "class_id"
                }
            }
        ],
        "execute": "one",
        "outputVariable": "data_class"
    }],
    "engine": []
}
