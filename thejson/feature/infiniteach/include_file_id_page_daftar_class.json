{
    "title": "Manage Class",
    "subtitle": "Manage Class",
    "frame": "adminpage",
    "id": "daftar_class",
    "ignore": false,
    "placement": [
        {
            "place": "sidemenu",
            "returnpackage": {}
        }
    ],
    "elemen": [
        {
            "type": "tabel",
            "id": "tabel_class",
            "title": "Class List",
            "columns": [
                "ID",
                "Class",
                "Description",
                "",
                "",
                ""
            ],
            "link": {
                "head": [
                    {
                        "position": "head",
                        "type": "modul_page",
                        "modul": "infiniteach",
                        "page": "add_class",
                        "label": "Add Class"
                    }
                ]
            },
            "listeners": [
                {
                    "listen": "onload",
                    "functions": [
                        {
                            "func_name": "get_all_accounts",
                            "func_type": "api_shooter",
                            "type": "api_shooter",
                            "content_generate": "auto",
                            "modul": "retrieve_data",
                            "action": "retrieve_classess",
                            "param": [],
                            "onAPIReturn": [
                                {
                                    "func_type": "json_extracter",
                                    "func_name": "extractJSONAccounts",
                                    "type": "json_extracter",
                                    "func_param": [
                                        "this_html_response",
                                        "\"response_data\""
                                    ],
                                    "variable": "hasilExtractJSONAccounts"
                                }
                                ,{
                                    "func_type": "change_datatable_by_json",
                                    "func_name": "set_isi_tabel_siswa",
                                    "type": "change_datatable_by_json",
                                    "func_param": [
                                        "hasilExtractJSONAccounts"
                                    ],
                                    "content_generate": "auto",
                                    "table_id": "tabel_class"
                                }
                            ]
                        }
                    ]
                }
            ],
            "forms": [
                {
                    "id": "form_cari_tabel",
                    "div_class": "box-header",
                    "attribute": {
                        "method": "post"
                    },
                    "field": [
                        {
                            "type": "select",
                            "id": "class_year",
                            "label": "Year",
                            "variable": "pilihan_class_year",
                            "class": "",
                            "theme": "normal",
                            "first_option_label": "- select -",
                            "first_option_value": "-1",
                            "attribute": {},
                            "value": {
                                "var_name": "daftar_year"
                            },
                            "listeners": [
                                {
                                    "listen": "onchange",
                                    "functions": [
                                        {
                                            "func_name": "get_accounts",
                                            "func_type": "api_shooter",
                                            "type": "api_shooter",
                                            "content_generate": "auto",
                                            "modul": "retrieve_data",
                                            "action": "retrieve_classess",
                                            "param": [
                                                {
                                                    "index": "class_year",
                                                    "slash": "",
                                                    "value": "pilihan_class_year"
                                                }
                                            ],
                                            "onAPIReturn": [
                                                {
                                                    "func_type": "json_extracter",
                                                    "func_name": "extractJSONFilteredAccount",
                                                    "type": "json_extracter",
                                                    "func_param": [
                                                        "this_html_response",
                                                        "\"response_data\""
                                                    ],
                                                    "variable": "hasilExtractJSONFilteredAccount"
                                                },
                                                {
                                                    "func_type": "change_datatable_by_json",
                                                    "func_name": "set_isi_tabel_class_by_filter",
                                                    "type": "change_datatable_by_json",
                                                    "func_param": [
                                                        "hasilExtractJSONFilteredAccount"
                                                    ],
                                                    "content_generate": "auto",
                                                    "table_id": "tabel_class"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "validation": []
                        }
                    ]
                }
            ]
        }
    ],
    "functions": [],
    "process": [{
        "type": "table",
        "from_engine": false,
        "runifnotnull": [],
        "table_name": "tb_class",
        "execute": "many",
        "process_name": "select",
        "output_generate": "auto",
        "param": [],
        "array_data": [
            "id",
            "class",
            {"raw":"YEAR(class_start) as class_start"}
        ],
        "where": [],
        "to_array": {
            "index": "class_start",
            "value": "class_start"
        },
        "outputVariable": "daftar_year"
    }],
    "engine": []
}
