{
    "title": "Manage Account",
    "subtitle": "Manage User Account",
    "frame": "adminpage",
    "id": "daftar_account",
    "ignore": false,
    "placement": [
        {
            "place": "sidemenu",
            "returnpackage": {}
        }
    ],
    "elemen": [
        {
            "type": "tabel",
            "id": "tabel_account",
            "title": "Account List",
            "columns": [
                "ID",
                "Name",
                "Email",
                "Role",
                "",
                ""
            ],
            "link": {
                "head": [
                    {
                        "position": "head",
                        "type": "modul_page",
                        "modul": "manage_account",
                        "page": "add_account",
                        "label": "Add Account"
                    }
                ]
            },
            "listeners": [
                {
                    "listen": "onload",
                    "functions": [
                        {
                            "func_name": "get_all_accounts",
                            "func_type": "api_shooter",
                            "type": "api_shooter",
                            "content_generate": "auto",
                            "modul": "retrieve_data",
                            "action": "retrieve_accounts",
                            "param": [],
                            "onAPIReturn": [
                                {
                                    "func_type": "json_extracter",
                                    "func_name": "extractJSONAccounts",
                                    "type": "json_extracter",
                                    "func_param": [
                                        "this_html_response",
                                        "\"response_data\""
                                    ],
                                    "variable": "hasilExtractJSONAccounts"
                                }
                                ,{
                                    "func_type": "change_datatable_by_json",
                                    "func_name": "set_isi_tabel_siswa",
                                    "type": "change_datatable_by_json",
                                    "func_param": [
                                        "hasilExtractJSONAccounts"
                                    ],
                                    "content_generate": "auto",
                                    "table_id": "tabel_account"
                                }
                            ]
                        }
                    ]
                }
            ],
            "forms": [
                {
                    "id": "form_cari_tabel",
                    "div_class": "box-header",
                    "attribute": {
                        "method": "post"
                    },
                    "field": [
                        {
                            "type": "select",
                            "id": "account_role",
                            "label": "Role",
                            "variable": "pilihan_account_role",
                            "class": "",
                            "theme": "normal",
                            "first_option_label": "- select -",
                            "first_option_value": "-1",
                            "attribute": {},
                            "value": {
                                "var_name": "ar_roles"
                            },
                            "listeners": [
                                {
                                    "listen": "onchange",
                                    "functions": [
                                        {
                                            "func_name": "get_accounts",
                                            "func_type": "api_shooter",
                                            "type": "api_shooter",
                                            "content_generate": "auto",
                                            "modul": "retrieve_data",
                                            "action": "retrieve_accounts",
                                            "param": [
                                                {
                                                    "index": "account_role",
                                                    "slash": "",
                                                    "value": "pilihan_account_role"
                                                }
                                            ],
                                            "onAPIReturn": [
                                                {
                                                    "func_type": "json_extracter",
                                                    "func_name": "extractJSONFilteredAccount",
                                                    "type": "json_extracter",
                                                    "func_param": [
                                                        "this_html_response",
                                                        "\"response_data\""
                                                    ],
                                                    "variable": "hasilExtractJSONFilteredAccount"
                                                },
                                                {
                                                    "func_type": "change_datatable_by_json",
                                                    "func_name": "set_isi_tabel_account_by_filter",
                                                    "type": "change_datatable_by_json",
                                                    "func_param": [
                                                        "hasilExtractJSONFilteredAccount"
                                                    ],
                                                    "content_generate": "auto",
                                                    "table_id": "tabel_account"
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "validation": []
                        }
                    ]
                }
            ]
        }
    ],
    "functions": [],
    "process": [],
    "engine": []
}
