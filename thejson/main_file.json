    ,{"moduls":"infiniteach","pages":["add_class","daftar_class","delete_class","edit_class","view_class"],"allow":[{
      "check":{
      "var_name": "_SESSION"
      ,"index":["auth_token"]},"operator":"!=","value":{
        "var_type":"hardcode"
      ,"var_name": "null"}
    }],"onfailed":{"process":[{"type":"change_page","modul":"account","page":"login"}]}}
    
    ],
