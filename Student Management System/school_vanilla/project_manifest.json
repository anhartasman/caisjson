    {"modul": "account",
    "action": [{
      "type": "crud_mysql",
      "action": "login",
      "param": [
        {
          "name": "email",
          "mandatory": true
        },
        {
          "name": "password",
          "mandatory": true
        }
      ],
      "process": [
        {
          "type": "table",
          "id": "tb_akun"
          ,"from_engine":true
          ,"param": [
            {
              "var_name": "param_api_email"
            },
            {
              "var_name": "param_api_password"
            }
          ],
          "runifnotnull": []
        }
        ,{
          "type": "table",
          "id": "tb_authlevel",
          "from_engine":true
          ,"param": [
            {
              "var_name": "hasil_get_akun"
              ,"index":["idauth"]
            }
            ,{
                "var_name": "hasil_get_akun"
              }
          ],
          "runifnotnull": [{
            "var_name": "hasil_get_akun"
          }]
        }
        ,{
          "type": "session"
          ,"direct":true
          ,"id": "set_session",
          "set_session": [
            {"session":"akun_email","value":{
              "var_name": "hasil_get_akun"
              ,"index":["email"]
            }}
            ,{"session":"akun_id","value":{
              "var_name": "hasil_get_akun"
              ,"index":["id"]
            }}
          ],
          "runifnotnull": [{
            "var_name": "hasil_get_akun"
          }]
        }
        ,{
          "type": "session"
          ,"direct":true
          ,"id": "check_session",
          "check_session": [
            {
              "check":{
              "var_name": "_SESSION"
              ,"index":["akun_email"]},"operator":"==","value":{
              "var_name": "hasil_get_akun"
              ,"index":["email"]}
            }
          ]
          ,"ontrue":{}
          ,"runifnotnull": [{
            "var_name": "hasil_get_akun"
          }]
        }
      ],
      "engine": [
        {
          "type": "table",
          "content": [
            {
              "table_name": "tb_akun",
              "id": "tb_akun",
              "param": [
                {
                  "name": "email"
                },
                {
                  "name": "password"
                }
              ],
              "process_name": "select",
              "array_data": [
                "id","email","password","idauth"
              ]
              ,"where": [
                {
                  "index": "tb_akun.email",
                  "operator": "=",
                  "value": {
                    "var_type": "variable",
                    "var_name": "email"
                  }
                }
                ,{
                  "index": "tb_akun.password",
                  "operator": "=",
                  "value": {
                    "var_type": "variable",
                    "var_name": "password"
                  }
                }
              ]
              ,"execute": "one",
              "outputVariable": "hasil_get_akun"
              ,"output_generate": "auto"

            }
            ,{
              "table_name": "tb_authlevel",
              "id": "tb_authlevel",
              "param": [
                {
                  "name": "id_auth"
                }
                ,{
                    "name": "hasil_get_akun"
                  }
              ],
              "process_name": "select",
              "array_data": [
                "id","level"
              ]
              ,"where": [
                {
                  "index": "tb_authlevel.id",
                  "operator": "=",
                  "value": {
                    "var_type": "variable",
                    "var_name": "id_auth"
                  }
                }
              ]
              ,"execute": "many",
              "outputVariable": "hasil_get_auth"
              ,"output_generate": "manual"
              ,"output": "{\"id\":\"v{'var_name':'hasil_get_akun','index':['id']}v\",\"email\":\"v{'var_name':'hasil_get_akun','index':['email']}v\",\"level\":\"v{'var_name':'qhasil_get_auth','index':['level']}v\"}"


            }
          ]
        }
      ],
      "response_output": "{hasil_get_auth}"
